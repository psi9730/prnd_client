    "react-router-redux": "^4.0.8",
    "react-scroll-area": "^0.1.2",
    "react-scroll-to": "^1.2.2",
    "react-scrollbar": "^0.5.4",
    "react-select": "^2.0.0-beta.6",
    "react-test-render": "^1.1.1",
    "react-touch-carousel": "^0.8.2",
    "reactstrap": "^6.0.1",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "redux-wait-for-action": "^0.0.7",
    "styled-components": "^1.4.6",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2",
    "validator": "^7.0.0",
    "whatwg-fetch": "^2.0.2"
  }
}
